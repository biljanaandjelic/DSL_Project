<?xml version="1.0" encoding="UTF-8"?>
<formValidation:ValidationClass
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:formValidation="http://www.example.org/formValidation"
    xsi:schemaLocation="http://www.example.org/formValidation formValidation.ecore"
    name="person">
  <classattribute
      name="name">
    <uicomponent
        xsi:type="formValidation:TextInput"
        tooltip="Please enter your name."
        placeHolder="Your name"/>
  </classattribute>
  <classattribute
      name="lastname"/>
</formValidation:ValidationClass>
