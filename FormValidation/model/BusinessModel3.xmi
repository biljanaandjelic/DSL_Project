<?xml version="1.0" encoding="UTF-8"?>
<formValidation:BusinessModel xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:formValidation="http://www.example.org/formValidation" xsi:schemaLocation="http://www.example.org/formValidation formValidation.ecore">
  <validationclass name="organizacionaJedinica" label="Organizaciona jedinica">
    <classattribute name="naziv" label="Naziv" isClassRepresenting="true">
      <attributetype>
        <uiComponent xsi:type="formValidation:TextInput"/>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
      <attributerule xsi:type="formValidation:Required">
        <attributeRuleErrorMessage message="Naziv organizacione jedinice je obavezno polje."/>
      </attributerule>
    </classattribute>
    <classattribute name="brojOrganizacioneJedinice" label="Broj organizacione jedinice">
      <attributetype>
        <uiComponent xsi:type="formValidation:TextInput"/>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
      <attributerule xsi:type="formValidation:Required"/>
      <attributerule xsi:type="formValidation:StringPattern" value="[0-9]{5}"/>
    </classattribute>
  </validationclass>
  <validationclass name="registracionaForma" label="Registraciona forma">
    <classattribute name="jedinstvenBrojKorisnikaJavnihSredstava" label="JBKJS" isClassRepresenting="true">
      <attributetype>
        <uiComponent xsi:type="formValidation:TextInput" note="Popunjava pravno lice/organ koji ima dodjeljen JBKJC"/>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
      <attributerule xsi:type="formValidation:Required">
        <attributeRuleErrorMessage message="JBKJS je obavezno polje."/>
      </attributerule>
    </classattribute>
    <classattribute name="nazivPravnogLicaOrgana" label="Naziv pravnog lica/organa" isClassRepresenting="true">
      <attributetype>
        <uiComponent xsi:type="formValidation:TextInput"/>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
    </classattribute>
    <classattribute name="maticniBroj" label="Matični broj">
      <attributetype>
        <uiComponent xsi:type="formValidation:TextInput"/>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
      <attributerule xsi:type="formValidation:Required"/>
      <attributerule xsi:type="formValidation:StringPattern" value="[0-9]{13}"/>
    </classattribute>
    <classattribute name="poreskiIdentifikacioniBroj" label="PIB">
      <attributetype>
        <datatype xsi:type="formValidation:PrimitiveDataType" type="Number"/>
      </attributetype>
      <attributerule xsi:type="formValidation:MinNumber" value="1.0E8"/>
      <attributerule xsi:type="formValidation:MinNumber" value="1.0E9"/>
      <attributerule xsi:type="formValidation:Required"/>
    </classattribute>
    <classattribute name="adresaSedista" label="Adresa sedista">
      <attributetype>
        <uiComponent xsi:type="formValidation:TextInput"/>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
    </classattribute>
    <classattribute name="adresaElektronskePoste" label="Adresa elektronske poste">
      <attributetype>
        <uiComponent xsi:type="formValidation:EmailInput"/>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
    </classattribute>
    <classattribute name="brojTelefona" label="Broj telefona">
      <attributetype>
        <uiComponent xsi:type="formValidation:TelInput"/>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
    </classattribute>
    <classattribute name="imeIPrezimeAdministratora" label="Ime i prezime" maser="false">
      <attributetype>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
    </classattribute>
    <classattribute name="brojIdentifikacioneIspraveAdministratora" label="Broj identifikacione isprave" maser="false">
      <attributetype>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
    </classattribute>
    <classattribute name="adresaElektronskePosteAdministratora" label="Adresa elektronske poste" maser="false">
      <attributetype>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
    </classattribute>
    <classattribute name="brojTelefonaAdministratora" label="Broj telefona" maser="false">
      <attributetype>
        <uiComponent xsi:type="formValidation:TelInput"/>
        <datatype xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
    </classattribute>
    <classattribute name="brojOrganizacioneJedinice" label="Broj organizacione jedinice">
      <attributetype>
        <uiComponent xsi:type="formValidation:AutocompleteComponent"/>
        <datatype xsi:type="formValidation:NonPrimitiveDataType" type="//@validationclass.0"/>
      </attributetype>
      <attributerule xsi:type="formValidation:Required"/>
    </classattribute>
    <classrule xsi:type="formValidation:NotEquals">
      <attributearray classattribute="//@validationclass.1/@classattribute.6 //@validationclass.1/@classattribute.10"/>
    </classrule>
    <formlayout xsi:type="formValidation:FieldSet" name="podaciOPravnomLicuOrganu" legend="Podaci o pravnom licu/organu">
      <formlayout xsi:type="formValidation:Grid" name="grid1.1" numOfColumns="2">
        <attributearray classattribute="//@validationclass.1/@classattribute.0"/>
      </formlayout>
      <formlayout xsi:type="formValidation:Grid" name="grid1.2">
        <attributearray classattribute="//@validationclass.1/@classattribute.1"/>
      </formlayout>
      <formlayout xsi:type="formValidation:Grid" name="grid1.3" numOfColumns="2">
        <attributearray classattribute="//@validationclass.1/@classattribute.2 //@validationclass.1/@classattribute.3"/>
      </formlayout>
      <formlayout xsi:type="formValidation:Grid" name="grid1.4" numOfColumns="1">
        <attributearray classattribute="//@validationclass.1/@classattribute.4"/>
      </formlayout>
      <formlayout xsi:type="formValidation:Grid" name="grid1.5">
        <attributearray classattribute="//@validationclass.1/@classattribute.5 //@validationclass.1/@classattribute.6"/>
      </formlayout>
    </formlayout>
    <formlayout xsi:type="formValidation:FieldSet" name="podaciOAdministratoru" legend="Podaci o administratoru">
      <formlayout xsi:type="formValidation:Grid" name="grid2.1" numOfColumns="1">
        <attributearray classattribute="//@validationclass.1/@classattribute.7"/>
      </formlayout>
      <formlayout xsi:type="formValidation:Grid" name="grid2.2" numOfColumns="2" expandLast="false">
        <attributearray classattribute="//@validationclass.1/@classattribute.8"/>
      </formlayout>
      <formlayout xsi:type="formValidation:Grid" name="grid2.3" numOfColumns="2">
        <attributearray classattribute="//@validationclass.1/@classattribute.9 //@validationclass.1/@classattribute.10"/>
      </formlayout>
    </formlayout>
    <formlayout xsi:type="formValidation:FieldSet" name="podaciOOrganizacionojJediniciUpraveZaTrezor" legend="Podaci o organizacionoj jedinici uprave za Trezor">
      <formlayout xsi:type="formValidation:Grid" name="grid3.1" numOfColumns="2">
        <attributearray classattribute="//@validationclass.0/@classattribute.1"/>
      </formlayout>
    </formlayout>
    <additionalSettings xsi:type="formValidation:ErrorMessageSettings" position="Below">
      <color red="255" green="33"/>
    </additionalSettings>
    <additionalSettings xsi:type="formValidation:LabelSettings" position="Below"/>
    <additionalSettings xsi:type="formValidation:NoteSettings" position="Below"/>
  </validationclass>
</formValidation:BusinessModel>
