<?xml version="1.0" encoding="UTF-8"?>
<formValidation:ValidationClass
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:formValidation="http://www.example.org/formValidation"
    xsi:schemaLocation="http://www.example.org/formValidation formValidation.ecore"
    name="Person">
  <classattribute
      name="attr1">
    <attributetype/>
    <attributerule
        xsi:type="formValidation:Required"
        isRequired="true"/>
    <attributerule
        xsi:type="formValidation:MaxLength"
        value="7"/>
    <attributerule
        xsi:type="formValidation:MinLength"
        value="4"/>
  </classattribute>
  <classattribute
      name="attr2">
    <attributetype
        vrsta="Number"/>
    <attributerule
        xsi:type="formValidation:Required"/>
  </classattribute>
  <classattribute
      name="attr3">
    <attributetype
        vrsta="Number"/>
    <attributerule
        xsi:type="formValidation:MaxNumber"
        value="2999.0"/>
    <attributerule
        xsi:type="formValidation:MinNumber"
        value="1000.0"/>
  </classattribute>
  <classattribute
      name="attr4">
    <attributetype
        vrsta="Number"/>
    <attributerule
        xsi:type="formValidation:Required"/>
  </classattribute>
  <classrule
      xsi:type="formValidation:GreaterThan"
      classattribute="//@classattribute.2"/>
  <classrule
      xsi:type="formValidation:Equals">
    <attributearray
        classattribute="//@classattribute.1 //@classattribute.2"/>
  </classrule>
  <classrule
      xsi:type="formValidation:LessThan"/>
</formValidation:ValidationClass>
