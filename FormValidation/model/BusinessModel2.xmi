<?xml version="1.0" encoding="UTF-8"?>
<formValidation:BusinessModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:formValidation="http://www.example.org/formValidation"
    xsi:schemaLocation="http://www.example.org/formValidation formValidation.ecore">
  <validationclass
      name="organizacionaJedinica"
      label="Organizaciona jedinica">
    <classattribute
        name="naziv"
        label="Naziv"
        isClassRepresenting="true">
      <attributetype>
        <uiComponent
            xsi:type="formValidation:TextInput"/>
        <datatype
            xsi:type="formValidation:PrimitiveDataType"/>
      </attributetype>
      <attributerule
          xsi:type="formValidation:Required"/>
    </classattribute>
    <classrule
        xsi:type="formValidation:GreaterThan">
      <attributearray/>
    </classrule>
  </validationclass>
</formValidation:BusinessModel>
